from catch22 import catch22_all

# Function to extract Catch22 features
def extract_catch22_features(eeg_data):
    # Assuming 'eeg_data' is a numpy array of shape (n_epochs, n_channels, n_samples)
    features = []
    for epoch in eeg_data:
        # Flatten the EEG data from each epoch into a single array (channel x samples)
        epoch_flat = epoch.flatten()
        c22_features = catch22_all(epoch_flat)
        features.append(c22_features)
    return np.array(features)

# Example of extracting Catch22 features
eeg_data = np.random.rand(10, 1, 1000)  # Example EEG data: 10 epochs, 1 channel, 1000 samples
catch22_features = extract_catch22_features(eeg_data)
print(catch22_features)
